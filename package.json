{
  "name"        : "mysql-live-select",
  "version"     : "1.0.0",
  "description" : "Live updating MySQL SELECT statements",
  "main"        : "lib/LiveMysql.js",
  "license"     : "MIT",
  "repository"  : "https://github.com/numtel/mysql-live-select.git",

  "contributors" : [
    "numtel <ben@latenightsketches.com>",
    "nothingisdead <rbmyr8@gmail.com>"
  ],
  "engines": {
    "node": "0.10"
  },
  "directories": {
