    "console.table"     : "0.4.0"
  },
  "dependencies": {
    "babel-runtime"  : "^4.7.12",
    "mysql"          : "^2.6.1",
    "zongji"         : "^0.3.2",
    "lodash"         : "^3.1.0",
    "murmurhash-js"  : "^1.0.0",
    "random-strings" : "^0.0.1"
